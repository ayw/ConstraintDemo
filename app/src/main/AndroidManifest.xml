<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zr.com.constraintdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NormalActivity" />
        <activity android:name=".ConstraintSetActivity" />
        <activity android:name=".GuidelineActivity" />
        <activity android:name=".normal.RelativePositionActivity" />
        <activity android:name=".normal.MarginActivity" />
        <activity android:name=".normal.CenterPositionActivity" />
        <activity android:name=".normal.BiasActivity"></activity>
    </application>

</manifest>